{
  "name": "evently",
  "version": "1.0.0",
  "main": "dist-electron/main/index.js",
  "description": "App to track Events !",
  "author": "WEIL Rémi <remi@remi-weil.fr>",
  "contributors": [
    "Théo SEMENTA"
  ],
  "license": "MIT",
  "private": true,
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ],
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && electron-builder --publish always",
    "preview": "vite preview",
    "icon": "electron-icon-builder --input=./public/logo.png --output=./",
    "sync:lang": "node sync-i18n.js && npm run check:lang",
    "check:lang": "node check-i18n.js"
  },
  "devDependencies": {
    "@types/luxon": "^3.4.2",
    "@vitejs/plugin-vue": "^5.0.4",
    "electron": "^29.1.1",
    "electron-builder": "^24.13.3",
    "electron-builder-notarize": "^1.5.2",
    "glob": "^11.0.1",
    "google-auth-library": "^9.15.1",
    "google-spreadsheet": "^4.1.4",
    "sass-embedded": "^1.83.4",
    "typescript": "^5.4.2",
    "vite": "^5.1.5",
    "vite-plugin-electron": "^0.28.4",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue": "^3.4.21",
    "vue-tsc": "^2.0.6"
  },
  "dependencies": {
    "@electron/notarize": "^2.5.0",
    "@intlify/unplugin-vue-i18n": "^6.0.3",
    "@mdi/font": "^7.4.47",
    "dotenv": "^16.4.7",
    "electron-updater": "^6.3.9",
    "luxon": "^3.5.0",
    "pinia": "^2.3.1",
    "qrcode.vue": "^3.6.0",
    "socket.io-client": "^4.8.1",
    "vite-svg-loader": "^5.1.0",
    "vue-apple-login": "^2.0.1",
    "vue-i18n": "^11.1.0",
    "vue-router": "^4.5.0",
    "vue3-lottie": "^3.3.1",
    "vuetify": "^3.7.9"
  }
}
